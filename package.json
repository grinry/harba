    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.22",
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-sass": "^3.3.7",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
